{
  "name": "enlighten",
  "version": "1.0.0",
  "description": "Instant Messenger written in ReactJS with Firebase backend",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./test/setup.js --require ignore-styles --require expect.js --recursive --no-colors --growl",
    "tdd": "npm test -- -R dot --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/easyCZ/enlighten.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "instant",
    "messenger",
    "firebase",
    "webpack",
    "babel"
  ],
  "author": "Milan Pavlik (http://github.com/easyCZ)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/easyCZ/enlighten/issues"
  },
  "homepage": "https://github.com/easyCZ/enlighten",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-register": "^6.5.2",
    "core-js": "^2.1.0",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "expect.js": "^0.3.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "ignore-styles": "^1.2.0",
    "jsdom": "^8.0.4",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.1",
    "react-addons-test-utils": "^0.14.7",
    "react-flexbox-grid": "^0.9.4",
    "redux-devtools": "^3.1.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "flexboxgrid": "^6.3.0",
    "material-design-icons": "^2.2.0",
    "normalize.css": "^3.0.3",
    "react": "^0.14.3",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "^0.14.3",
    "react-redux": "^4.4.0",
    "react-toolbox": "^0.14.1",
    "redux": "^3.3.1"
  }
}
